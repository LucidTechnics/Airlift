<queue-entries>
	<total-storage-limit>10G</total-storage-limit>

	<queue>
		<name>ExampleQueue</name>
		<rate>500/s</rate>
		<bucket-size>100</bucket-size>
		<max-concurrent-requests>100</max-concurrent-requests>
		<retry-parameters>
			  <task-retry-limit>5</task-retry-limit>
			  <task-age-limit>60m</task-age-limit>
			  <min-backoff-seconds>60</min-backoff-seconds>
			  <max-backoff-seconds>120</max-backoff-seconds>
			  <max-doublings>2</max-doublings>
		</retry-parameters>
	</queue>

</queue-entries>